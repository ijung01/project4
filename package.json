{
  "name": "",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:sass && eleventy",
    "start": "npm run watch:sass && eleventy --serve",
    "build:sass": "sass --load-path='./node_modules' --no-source-map sass/styles.scss:css/styles.css",
    "watch:sass": "sass --load-path='./node_modules' --no-source-map --watch sass/styles.scss:css/styles.css"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "sass": "^1.27.2"
  },
  "license": "MIT"
}
